{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "standard",
    "prettier",
    "prettier/standard"
  ],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "prettier/prettier": "error",
    "linebreak-style": 0
  },
  "plugins": [
    "prettier"
  ]
}
