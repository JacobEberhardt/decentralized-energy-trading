  "scripts": {
    "compile-contracts": "truffle compile",
    "connect-docker-nodes": "node scripts/connect_nodes.js",
    "migrate-contracts": "truffle migrate",
    "test-contracts": "run-script-os",
    "test-contracts:linux:darwin": "truffle test ./test/contracts/*",
    "test-contracts:win32": ".\\test-contracts.bat",
    "run-server": "node ./household-server/index.js",
    "run-sensor": "node ./mock-sensor/index.js",
    "run-netting-entity": "node ./netting-entity/index.js",
    "run-benchmark-ned": "node ./ned-server/benchmarkNed.js",
    "lint-all": "eslint *.js household-server/ mock-sensor/ test/ helpers/ household-ui/src utility-js/ scripts/ migrations/ && yarn lint-contracts",
    "lint-contracts": "solium -d contracts/",
    "format-all": "eslint *.js household-server/ mock-sensor/ test/ helpers/ household-ui/src utility-js/ scripts/ migrations/ --fix && yarn format-contracts",
    "format-contracts": "solium -d contracts/ --fix",
    "run-ganache": "ganache-cli --account='0xdcb15ba5d2caf586c22f0414f527201d2fb2424c92ced3efacd742a34e5b0db2,100000000000000000000' -i 1234 -l 0x1fffffffffffff",
    "migrate-contracts-ganache": "truffle migrate --reset hard --network ganache",
    "migrate-contracts-authority": "truffle migrate --reset hard --network authority --f 2",
    "migrate-contracts:benchmark": "truffle migrate --reset hard --network benchmark",
    "test-parity-docker": "./scripts/run_docker_parity_tests.sh",
    "test-helpers": "mocha ./test/helpers/*.js",
    "test-utility-js": "mocha ./test/utility-js/*",
    "update-contract-bytecodes": "yarn compile-contracts && node ./scripts/update-contract-bytecodes",
    "setup-zokrates": "./scripts/setup_zokrates.sh",
    "run-benchmark-node": "./benchmark/run-parity.sh",
    "generate-proving-files": "node zokrates-code/zoKratesCodeGenerator.js",
    "generate-docker-parity": "node generateDockerChain.js",
    "docker-parity-setup": "./parity-authority/parity_setup.sh",
    "setup-system": "./scripts/setup_script.sh",
    "generate-docker-files": "node setup_script.js"

  },
  "devDependencies": {
    "@gnosis.pm/mock-contract": "3.0.7",
